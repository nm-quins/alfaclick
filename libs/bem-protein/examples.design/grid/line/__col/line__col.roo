.line__col {
    padding-left: 10px;
    line-height: 2rem;

    &:nth-child(1) {
        background: #1abc9c;
    }

    &:nth-child(2) {
        background: #2ecc71;
    }

    &:nth-child(3) {
        background: #3498db;
    }

    &:nth-child(4) {
        background: #9b59b6;
    }

    &:nth-child(5) {
        background: #34495e;
    }

    &:nth-child(6) {
        background: #f1c40f;
    }

    &:nth-child(7) {
        background: #e67e22;
    }

    &:nth-child(8) {
        background: #e74c3c;
    }

    &:nth-child(9) {
        background: #95a5a6;
    }

    &:nth-child(10) {
        background: #f39c12;
    }

    &:nth-child(11) {
        background: #d35400;
    }

    &:nth-child(12) {
        background: #c0392b;
    }
}
